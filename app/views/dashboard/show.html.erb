<div class= "row">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">Appliance With Targtes</h3>
    </div>
    <div class="panel-body">
     <table class="table ">
      <tr>
      <th>Name:</th>
        <td><%=@appliance.name%></td>
      </tr>
      <tr>
        <th>Customer:</th>
        <td><%=@appliance.customer%></td>
      </tr>
      <tr>
        <th>Targets:</th>
        <%@appliance.targets.each do |target| %>
        <td class="small">
            <div><label>HostName:</label><%=target.hostname%></div>
            <div><label>Address:</label><%=target.address%></div>
            <div><label>Status:</label><%= target.status%></div>
        </td>
        <%end%>

        </tr>
      </table>
        <div class=""> <%= link_to "Back",root_path ,class:'btn btn-primary btn-sm'%></div>
  </div>
</div>
